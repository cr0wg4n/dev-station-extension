{
  "name": "Dev Station",
  "description": "a default tool for developers",
  "version": "1.0",
  "manifest_version": 3,
  "action":{
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "permissions": [
    "webRequest",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons":{
    "128": "icon.png"
  }
}