{
  "name": "Dev Station",
  "description": "The definitive Swiss Knife extension for developers!",
  "version": "0.0.0.2",
  "manifest_version": 3,
  "action":{
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "src/background.ts"
  },
  "icons":{
    "128": "icon.png"
  },
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "author": "Mauricio Matias"
}